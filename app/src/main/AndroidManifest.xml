<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="griffinbean.example.com.securepollmobile">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
            android:allowBackup="true"
            android:icon="@mipmap/securepoll"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".Controller.HomePageActivityController">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Controller.LoginActivityController"></activity>
        <activity android:name=".Controller.RegisterActivityController"></activity>
        <activity android:name=".Controller.TwoFacAuthActivityController"></activity>
        <activity android:name=".Controller.RaceListActivityController"></activity>
        <activity android:name=".Controller.CampaignItemActivityController"></activity>
        <activity android:name=".Controller.ChangePasswordActivityController"></activity>
        <activity android:name=".Controller.CampaignResultActivityController"></activity>
        <activity android:name=".Controller.DeactivateAccountActivityController"></activity>
    </application>

</manifest>